{
  "name": "ts-kata",
  "version": "1.0.0",
  "description": "Typescript kata starting project",
  "author": "Pedro Moreira Santos",
  "homepage": "https://github.com/pedromsantos/ts-kata",
  "keywords": [
    "kata",
    "dojo",
    "code",
    "practice",
    "tdd"
  ],
  "license": "MIT",
  "main": "./lib/kata.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "watch": "jest --watch",
    "clean": "rm -r ./lib",
    "test": "jest --watchAll=false",
    "precommit": "npm-run-all -p lint types test build",
    "test:ci": "jest test --watchAll=false --coverage=true",
    "mutants": "stryker run",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --color --ext .ts --ignore-pattern 'node_modules/'",
    "lint:fix": "eslint . --ext .ts --ignore-pattern 'node_modules/' --fix",
    "types": "type-coverage -at-least 99",
    "types-report": "typescript-coverage-report --threshold 99",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@stryker-mutator/core": "^6.3.0",
    "@stryker-mutator/jest-runner": "^6.3.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "fast-check": "^3.3.0",
    "husky": "^8.0.2",
    "jest": "^28.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-coverage": "^0.1.5",
    "ts-jest": "^28.0.8",
    "type-coverage": "^2.23.0",
    "typescript": "^4.9.3",
    "typescript-coverage-report": "^0.7.0"
  }
}
